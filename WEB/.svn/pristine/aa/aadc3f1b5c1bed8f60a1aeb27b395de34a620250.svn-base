.style1{
    margin:0.5rem 0 0;
    padding:0 0.2rem 0.3rem
}
.style1 ul li{
    width:33.3%;
    text-align: left;
}
.style1 ul li p:nth-child(2){
    line-height: 0.5rem;
    height:0.5rem;
}
.style1 ul li b{
    display: inline-block;
    float:left;
    font-size: 0.3rem;
    height:0.5rem;
    line-height: 0.5rem;
    color:#000;
    letter-spacing: 0.03rem;
    margin-right: 0.05rem;
}

.style2 .title{
    height:0.8rem;
    line-height: 0.5rem;
    font-size: 0.2rem;
    border-top:1px solid #eeeeee;
    padding-top:0.3rem;
    margin:0 0.2rem;
    width:calc(100% - 0.4rem);

}
.style2 .title span:first-child{
    float:left;
    display: inline-block;
    line-height: 0.5rem;
}
.style2 .title span:last-child{
    float:right;
    display: inline-block;
    line-height: 0.5rem;
    width:0.8rem;
    text-align: center;
}

.style2-ul2{
    height: 3.13rem;
}
.style2-ul2 li{
    width:100%;
    display: block;
    height:0.48rem;
    line-height: 0.48rem;
    color:#666;
    text-align: left;
    padding:0 0.2rem;
    margin:0.05rem 0;
}
.style2-ul2 li span:first-child{
    display: inline-block;
    width:3rem;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.style2-ul2 li span:last-child{
    float:right;
