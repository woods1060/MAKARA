    width:11.2rem;

}
.floor6-right{
    width:calc(100% - 11.2rem);

}
.style4{
    padding:0 0.35rem;
}
.style4 ul li{
    height:1.9rem;
    padding:0.35rem 0;
    border-bottom: 1px solid #eee;
}
.style4 ul li img{
    height:1.2rem;
    width:1.6rem;
    border-bottom: 0.1rem;
    margin-right: 0.3rem;
    float:left;
}
.style4 ul li .text{
    float:left;
    width:7.7rem;
}

.style4 ul li .text .text-title{
    font-size: 0.24rem;
    color:#333;
    font-weight: bold;
    line-height: 0.35rem;
}
.style4 ul li .text .text-title:after{
    content: '';
    display: block;
    width:0.45rem;
    height:0.03rem;
    background-color: #2585f7;
}
.style4 ul li .text p:last-child{
    margin:0.2rem 0;
    color:#666;
    line-height: 0.26rem;
    font-size: 0.14rem;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.style4 ul li .style4-date{
    width:1.6rem;
    float:right;
    text-align: right;
    margin-right: 0.9rem;
    font-size: 0.14rem;
    color:#318cf7;
    height:1.3rem;
    background: url(../images/enter.png) right bottom no-repeat;
    background-size: 0.2rem 0.17rem;
}
.style4 .pages{
    text-align: center;
    margin-top: 0.5rem;
}
.style4 .pages a{
    margin:0 0.1rem;
}
.style4 .pages a.active{
    color:#67a2f8;
}


.style5{
    padding:0.2rem 0.3rem 0;
}
.style5 ul li{
    width:3.6rem;
    height:1.45rem;
    background-color: #f8f8f8;
    border-radius: 0.2rem;
    position: relative;
    margin:0 0 0.25rem;
    padding:0.2rem 0.2rem 0.1rem 0.4rem;
}
.style5 ul li .date{
    position: absolute;
    width:0.6rem;
    height:0.6rem;
    left:-0.3rem;
    top:50%;
    margin-top: -0.3rem;
    background-color: #2585f7;
    color:#fff;
    text-align: center;
    border-radius: 0.1rem;
    padding:0.05rem 0.1rem;
}
.style5 ul li .date p{
    line-height: 0.25rem;
    border-bottom: 0.02rem solid #fff;
}
.style5 ul li .date p:last-child{
    border-bottom: none;
}

.style5 ul li p{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    font-size: 0.14rem;
}
.style5 ul li p.title{
    font-size: 0.18rem;
    line-height: 0.25rem;
    color:#333;
    font-weight: bold;
    margin-bottom: 0.15rem;
}
.chart{
    width:100%;
    height:calc(100% - 0.8rem)
}



.style6{
    padding:0.1rem 0.2rem 0;
}
.style6 ul li{
    width:100%;
    height:0.8rem;
    padding:0.1rem 0.3rem;
    padding-right:0;
    border-left: 1px solid #f1f7fe;
    position: relative;
}
.style6 ul li:before{
    content: '';
    display: inline-block;
    width:0.1rem;
    height:0.1rem;
    position: absolute;
    left:-0.07rem;
    top:0.15rem;
    border:0.02rem solid #2e90f7;
    background-color: #fff;
    border-radius: 50%;
}
.style6 ul li:hover:before{
    left:-0.09rem;
    border:0.04rem solid #2589f7;
    background-color: #fff;
    border-radius: 50%;
}
.style6 ul li .text{
    float:left;
    width:1.6rem;
    height:0.6rem;
    font-size: 0.14rem;
}
.style6 ul li .text p{
    line-height: 0.3rem;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.style6 ul li .text p:nth-child(2){
    color:#666;
}

.style6 ul li .btn{
    float:right;
    width:0.86rem;
    height:0.28rem;
    line-height: 0.28rem;
    color:#fff;
    text-align: center;
    font-size: 0.14rem;
    border-radius: 0.2rem;
    margin-top: 0.1rem;
}

.style6 ul li .status1{
    background-color: #59c57f;
}

.style6 ul li .status2{
    background-color: #f28f3d;
}
