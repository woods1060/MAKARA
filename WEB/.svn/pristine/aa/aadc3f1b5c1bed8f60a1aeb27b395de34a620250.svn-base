.style3{
    padding:0 0.3rem;
}
.style3 ul li{
    line-height: 0.5rem;
}

.style3 ul li p{
    height:0.5rem;
    line-height: 0.5rem;
}

.style3 ul li p span:first-child{
    display: inline-block;
    width:3rem;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.style3 ul li p span:last-child{
    float:right;
    display: inline-block;
    width:0.3rem;
    height:0.3rem;
    margin-top: 0.1rem;
}
.style3 ul li.status1 .status{
    background:url(../images/status1.png) center no-repeat ;
    background-size: 0.3rem;
}
.style3 ul li.status2 .status{
    background:url(../images/status2.png) center no-repeat ;
    background-size: 0.3rem;
}
.style3 ul li.status3 .status{
    background:url(../images/status3.png) center no-repeat ;
    background-size: 0.3rem;
}
.style3 ul li.status3 p span:first-child{
    color:red;
}
.style3 ul li p.tip{
    height:0.4rem;
    line-height: 0.4rem;
    margin:0.05rem 0;
}
.style3 ul li.status2 p.tip{
    background-color: #fdebcf;
    border-radius: 0.25rem;
    border: 1px solid #f7bd5e;
    padding:0 0.2rem;
}
.style3 ul li.status3 p.tip{
    background-color: #f8cdce;
    border-radius: 0.25rem;
    border: 1px solid red;
    padding:0 0.2rem;
    color:red;
}
