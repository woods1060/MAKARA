}
.floor1-left-top ul li:nth-child(2) p:nth-child(2){
    color:#53b437;
}
.floor1-left-top ul li:nth-child(3) p:nth-child(2){
    color:#ee595a;
}

.floor1-left-top ul li p:nth-child(3){
