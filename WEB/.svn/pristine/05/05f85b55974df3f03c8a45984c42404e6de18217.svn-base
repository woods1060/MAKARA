        $("#detail1 ul").html(ul);




        $("#table1 tbody tr").off().on("click",function(){



            var sjs=$(this).index();

            var detail1=dataArr[sjs]

                var ul=''
                for(var i=0;i<detail1.length;i++){
                    var li='<li><a href="javascript:"><div class="text"><p>'+detail1[i].name+'</p><p>办理中：'+detail1[i].value+'天</p></div><div class="btn status'+detail1[i].status+'">正在办理</div></a></li>'
                    ul+=li
                }

                $("#detail1 ul").html(ul);


        })

    }





